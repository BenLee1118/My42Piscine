#include <stdio.h>

int		ft_strlen(char *str)
{
	int i;

	i = 0;
	while (str[i])
		i++;
	printf("%d", i);
	return(i);
}

int		main(void)
{
	ft_strlen("thishas8");
	return(0);
}
